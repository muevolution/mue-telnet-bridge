{
  "name": "@muevolution/telnet-bridge",
  "version": "1.0.0",
  "description": "MUE Telnet Bridge",
  "author": "Kauko <kauko@biosynth.link>",
  "license": "MIT",
  "main": "build/app.js",
  "scripts": {
    "start": "node build/app.js",
    "debug": "node --inspect build/app.js",
    "build": "npm-run-all build:ts",
    "build:ts": "tsc -b .",
    "build:watch": "tsc -b . --watch",
    "build:clean": "rm -rf build",
    "build:lint": "tslint -p .",
    "build:run": "concurrently \"npm run build:watch\" \"nodemon\""
  },
  "dependencies": {
    "@muevolution/client-types": "0.0.1",
    "bluebird": "^3.5.4",
    "lodash": "^4.17.11",
    "nconf": "^0.10.0",
    "socket.io-client": "^2.2.0",
    "source-map-support": "^0.5.11",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/lodash": "^4.14.123",
    "@types/nconf": "^0.10.0",
    "@types/node": "^11.13.0",
    "@types/socket.io-client": "^1.4.32",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1"
  }
}
